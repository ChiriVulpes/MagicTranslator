{
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "*": [
                "*"
            ]
        },
        "forceConsistentCasingInFileNames": true,
        "module": "commonjs",
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": false,
        "strict": true,
        "strictPropertyInitialization": false,
        "target": "esnext",
        "types": [],
        "plugins": [
            {
                "name": "typescript-tslint-plugin"
            }
        ]
    },
    "include": [
		"**/*.ts"
    ],
    "version": "2.2.2"
}
